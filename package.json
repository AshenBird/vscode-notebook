{
	"name": "notebook",
	"displayName": "McSwift Notebook",
	"description": "",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.63.1"
	},
	"publisher": "McSwift",
	"categories": [
		"Other",
		"Extension Packs"
	],
	"main": "./out/host/index.js",
	"activationEvents": [
		"onStartupFinished"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "mcswift-notebook",
					"title": "Notebook",
					"icon": "public/notebook.svg"
				}
			]
		},
		"views": {
			"mcswift-notebook": [
				{
					"id": "mcswift-note-explorer",
					"name": "目录",
					"type": "webview"
				}
			]
		}
	},
	"extensionPack": [
		"tomoki1207.pdf",
		"hediet.vscode-drawio",
		"gera2ld.markmap-vscode",
		"csholmq.excel-to-markdown-table",
		"vscode-icons-team.vscode-icons",
		"mcswift.markdown"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/AshenBird/vscode-note"
	},
	"scripts": {
		"pub": "vsce publish --yarn",
		"vscode:prepublish": "npm run build",
		"build": "node scripts/build.js",
		"watch": "node scripts/watch.js",
		"build:client": "cd src/client/explorer && cross-env MODE=build vite build",
		"watch:host": "cd src/host && tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.1",
		"@types/glob": "^8.0.1",
		"@types/mocha": "^10.0.1",
		"@types/node": "~18.11.19",
		"@types/vscode": "^1.75.0",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"@vicons/fa": "^0.12.0",
		"@vicons/tabler": "^0.12.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitejs/plugin-vue-jsx": "^3.0.0",
		"@vscode/test-electron": "^2.2.3",
		"@vscode/vsce": "^2.17.0",
		"@vueuse/head": "^1.0.24",
		"cross-env": "^7.0.3",
		"esbuild": "^0.17.5",
		"eslint": "^8.33.0",
		"execa": "^6.1.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"sass": "^1.58.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.1",
		"vue-router": "^4.1.6",
		"zx": "^7.1.1"
	},
	"dependencies": {
		"@vicons/ionicons5": "^0.12.0",
		"@vueuse/core": "^9.12.0",
		"fs-extra": "^11.1.0",
		"naive-ui": "^2.34.3",
		"vfonts": "^0.0.3",
		"vue": "^3.2.47"
	}
}